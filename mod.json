{
	"geode": "5.0.0-beta.3",
	"gd": {
		"win": "2.2081",
		"android": "2.2081",
		"mac": "2.2081",
		"ios": "2.2081"
	},
	"id": "ryxu-xo.ryxu_mod",
	"name": "Ryxu Mod",
	"version": "v1.0.0",
	"developer": "ryxu-xo",
	"description": "A mini mod menu for geometry dash",
	"settings": {
		"ryxu-title-core": {
			"type": "title",
			"name": "Ryxu Mod - Core"
		},
		"fps-bypass-enabled": {
			"type": "bool",
			"name": "Enable FPS Bypass",
			"description": "Unlock custom frame cap",
			"default": true
		},
		"fps-bypass": {
			"type": "int",
			"name": "FPS Cap",
			"description": "Target FPS for animation interval",
			"default": 240,
			"min": 30,
			"max": 1000,
			"controls": {
				"slider": true,
				"slider-step": 5,
				"arrows": true,
				"arrow-step": 5,
				"input": true
			},
			"enable-if": "fps-bypass-enabled"
		},
		"show-counters": {
			"type": "bool",
			"name": "Show FPS & CPS Counter",
			"description": "Display the overlay in levels",
			"default": true
		},
		"rgb-fps": {
			"type": "bool",
			"name": "RGB FPS",
			"description": "Rainbow color effect for FPS label",
			"default": true,
			"enable-if": "show-counters"
		},
		"rgb-cps": {
			"type": "bool",
			"name": "RGB CPS",
			"description": "Rainbow color effect for CPS label",
			"default": false,
			"enable-if": "show-counters"
		},
		"unlock-all-icons": {
			"type": "bool",
			"name": "Unlock All Icons",
			"description": "Treat icon unlock checks as unlocked",
			"default": false
		},
		"practice-music-sync": {
			"type": "bool",
			"name": "Practice Music Sync",
			"description": "Keep practice music synced to level audio",
			"default": true
		},
		"ryxu-title-pro": {
			"type": "title",
			"name": "Ryxu Mod - Pro"
		},
		"noclip": {
			"type": "bool",
			"name": "Noclip",
			"description": "Ignore death collisions",
			"default": false
		},
		"show-hitboxes": {
			"type": "bool",
			"name": "Show Hitboxes",
			"description": "Enable debug collider visualization hook",
			"default": false
		},
		"auto-checkpoints": {
			"type": "bool",
			"name": "Auto-Checkpoints",
			"description": "Automatically place checkpoints after reset",
			"default": false
		}
	},
	"dependencies": {
		"geode.node-ids": ">=v1.20.0"
	}
}
